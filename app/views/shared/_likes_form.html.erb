<%# end %>

<!--app/views/users/_likes_form.html.erb-->

<#% if current_user != @user %>
  <div id="likes_form">
  <% if current_user.like?(@user) %>
    <%= render 'unlike' %>
  <% else %>
    <%= render 'like' %>
  <% end %>
  </div>
<% end %>